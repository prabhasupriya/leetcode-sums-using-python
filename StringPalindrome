class Solution:
    def isPalindrome(self, s: str) -> bool:
        s = "".join(c.lower() for c in s if c.isalnum())
        #return s == s[::-1]
        l = 0
        right = len(s)-1
        while(l < right):
            if s[l] != s[right]:
                return False
            l +=1
            right -=1
        return True
